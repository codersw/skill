/* pages/huangli/huangli.wxss */
page {
    background-color: white;
}

.date {
    font-weight: bold;
    justify-content: center;
    font-size: 22px;
    flex-direction: row;
    display: inline-block;
    text-align: left;
    height: 22px;
    line-height: 22px;
    padding: 20rpx;
}

.candt {
    display: inline-block;
    position: absolute;
    top: 20rpx;
    left: 250rpx;
    font-size: 36rpx;
    height: 22px;
    line-height: 22px;
    font-weight: 500;
}
.container {
    display: flex;
    flex-direction: column;
    margin: 10rpx;
}

.calendar-main {
    margin-bottom: 10rpx;
    margin-left: 10rpx;
}

.calendar-title {
    text-align: center;
    vertical-align: middle;
}

.arrow {
    float: none;
    width: 16px;
    height: 9px;
    padding-left: 10rpx;
    vertical-align: middle;
}

.calendar-dayView {
    display: flex;
    flex-direction: column;
    text-align: center;
}

.calendar-day {
    font-family: impact;
    font-size: 150px;
    font-weight: bold;
    line-height: 135px;
    padding: 0;
    color: seagreen;
}

.calendar-nongli {
    font-size: 18px;
    font-weight: 500;
    color: seagreen;
}

.calendar-week {
    font-size: 18px;
    font-weight: 500;
    color: seagreen;
}

.font-medium {
    font-size: 15px;
}

.calendar-detail {
    margin-bottom: 5px;
    padding: 2px;
    border: 2px solid seagreen;
    border-radius: 10rpx 10rpx;
}

.color-green {
    font-weight: bold;
    color: seagreen;
}

.calendar-table {
    display: table;
    width: 100%;
    border-collapse: collapse;
    border: 1px solid seagreen;
    border-bottom: none;    
    height: 100px;
}

.row {
    display: table-row;
    vertical-align: middle;
}

.cell {
    display: table-cell;
    width: 33%;
    height: 450rpx;
    vertical-align: middle;
    border-right: 1px solid seagreen;
}

.cell-img {
    display: block;
    width: 200rpx;
    height: 200rpx;
    margin-top: -20px;
}

.cell-left1 {
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 80rpx;
    vertical-align: middle;
    border-bottom: 1px solid seagreen;
    align-items: center;
}
.content-max {
    padding: 5px 5px;
    word-wrap:break-word;
    width: 98%;
}

.content-name {
    word-wrap:break-word;
    font-weight: 700;
    font-size: 12px;
}
  
.content-txt {
    word-wrap:break-word;
    font-size: 10px;
    color:blue;
 }
  
.cell-left2 {
    display: flex;
    flex-direction: column;
    height: 450rpx;
    align-items: center;
}

.cell-middle1 {
    display: flex;
    flex-direction: column;
    height: 210rpx;
    align-items: center;
}

.cell-middle2 {
    padding: 3px;
    width: 100%;
}
.cell-middle2 text{
    color: blue;
    font-size: 12px;
}

.circle-bgc {
    height: 25px;
    width: 25px;
    margin: 8rpx;
    padding: 8rpx;
    border-radius: 58%;
    background-color: seagreen;
}

.circle-1 {
    display: block;
    padding: 2rpx;
    border: 1px solid seagreen;
    border-radius: 50%;
}

.font-circle {
    font-size: 18px;
    color: #fff;
    padding: 4px;
}

.yi {
    width: 100%;
}

.yi  .text1 {
    float: left;
    width: 100%;
}
.yi  .text2 {
    float: left;
    color: blue;
    font-size: 10px;
}

.cell-1 {
    width: 20%;
    height: 185rpx;
    text-align: center;
}

.cell-2 {
    width: 60%;
    height: 166rpx;
    text-align: center;
}

.cell-3 {
    width: 40%;
    height: 150rpx;
    text-align: center;
}

.cell-4 {
    width: 20%;
    height: 150rpx;
    text-align: center;
}

.table {
    display: table;
    width: 100%;
}

.tr {
    display: table-row;
}

.td {
    display: table-cell;
}

.table-3 .cell > view:first-child {
    height: 50rpx;
}

.table-3 .cell > view:last-child {
    height: 100rpx;
}
.table-3{
    border-bottom: 1px solid seagreen;
}
